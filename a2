-- Ensure you have Fluxus Executor running to inject this script

-- Load Dex Explorer into the game
local Dex = Instance.new("ScreenGui") -- Create a new ScreenGui instance

-- Create a frame for Dex Explorer
local frame = Instance.new("Frame")
frame.Size = UDim2.new(0.5, 0, 0.5, 0) -- Adjust the size as needed
frame.Position = UDim2.new(0.25, 0, 0.25, 0) -- Center the frame
frame.BackgroundColor3 = Color3.new(0, 0, 0) -- Set the background color to black
frame.Parent = Dex -- Parent the frame to Dex

-- Add some basic elements (e.g., a title)
local title = Instance.new("TextLabel")
title.Text = "Dex Explorer"
title.Size = UDim2.new(1, 0, 0.1, 0) -- Size of the title
title.BackgroundColor3 = Color3.new(1, 0, 0) -- Red background for the title
title.Parent = frame -- Parent the title to the frame

-- Make the Dex Explorer visible
Dex.Parent = game:GetService("CoreGui")
Dex.Enabled = true

print("Dex Explorer has been loaded!")
